									<div class="grievCont">
										<div class="boxGriev d-flex">
											<div class="boxIcon griev1">
												<img src="/griev_reg.png" width="75px" alt="user">
											</div>
											<div class="box-title">
												<h3>0</h3>
											</div>
										</div>
										<div class="card-footer">
											<div class="stats">
												<span>Total Grievance Registered</span>	
											</div>
										</div>
									</div>
								</a>
							</div>
							<div  class="col-lg-4 col-md-6 col-sm-6 contentBox">
								<a href="#" id="Pending" style="text-decoration: none">
									<div class="grievCont">
										<div class="boxGriev d-flex">
											<div class="boxIcon griev2">
												<img src="/griev_total.png" width="75px" alt="user">
											</div>
											<div class="box-title">
												<h3>0</h3>
											</div>
										</div>
										<div class="card-footer">
											<div class="stats">
												<span>Number of Grievances Pending</span>
											</div>
										</div>
									</div>
								</a>
							</div>
							<div class="col-lg-4 col-md-6 col-sm-6 contentBox">
								<a href="#" id="Closed" style="text-decoration: none">
									<div class="grievCont">
										<div class="boxGriev d-flex">
											<div class="boxIcon griev3">
												<img src="/griev_closed.png"width="75px" alt="user">
											</div>
											<div class="box-title">
												<h3>0</h3>
											</div>
										</div>
										<div class="card-footer">
											<div class="stats">
												<span>Number of Grievance Closed</span>
											</div>
										</div>
									</div>
								</a>
							</div>
						</div>
							<div class="card card-info">
    							<div class="card-header" style="background-color: #48a3b8">
        							<h4 class="card-title">List of Grievances</h4>
    							</div>
    							<div class="card-body">
       								<table id="TblGrievance" class="table table-striped table-bordered" style="width: 100%">
            							<thead>
            							<tr class="bg-secondary">
                							<th>Sn.</th>
                							<th>Registration Number</th>
                							<th>Received Date</th>
                							<th>Grievance description</th>
                							<th>Status</th>
                							<th>&nbsp;</th>
            							</tr>
            							</thead>
            							<tbody>
            							</tbody>
